for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.ItemShop.Items:GetChildren()) do
	if v.Name == "ItemShop_Template" ten
		v:Destroy()
	end
end	
for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.ItemShop.Items:GetChildren()) do
	if v:IsA("TextButton") then
		v.Parent = game.Workspace
	end
end	

game:GetService("Players").LocalPlayer.PlayerGui.MainUI.ItemShop.Items:Destroy()
