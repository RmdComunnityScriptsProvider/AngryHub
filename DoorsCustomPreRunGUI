for i,v in pairs(game.Players.LocalPlayer.PlayerGui.MainUI.ItemShop.Items:GetChildren()) do
	if v:IsA("TextButton") then
		v.Parent = game.Workspace
	end
end
for i,v in pairs(game.Workspace:GetChildren()) do
	if v.Name == "ItemShop_Template" then
		v:Destroy()
	end
end
game.Players.LocalPlayer.PlayerGui.MainUI.ItemShop.Items:Destroy()

